<h1>Questions</h1>
<div class="list-group">
<% @questions.each do |question| %>
  <%= link_to(question, class: "list-group-item list-group-item-action") do %>
    <h5><%= question.title %></h5>
    <p><%= question.user.name %></p>
    <small><%= question.created_at %></small>
  <% end %>
<% end %>
</div>
